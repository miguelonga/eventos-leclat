<div class="9u 12u(medium) 12u(small) important(medium)">
	<div id="content">

		<!-- Content -->

				<h2><%= @breadcrumb %></h2>
				<div class="row">
					<% @products.each do |product| %>
							<div class="6u 6u(medium) 12u(small)">
								<section class="box feature">
									<a href="/catalogo/ver/<%= product["name"] %>" class="image featured"><img src="<%= product["images"].first %>" alt="" /></a>
									<div class="inner">
										<header>
											<h4><%= product["name"] %></h4>
											<p><%= product["price"] %></p>
										</header>
									</div>
								</section>
							</div>
					<% end %>
				</div>

	</div>
</div>